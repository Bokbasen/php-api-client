{
	"name" : "bokbasen/php-api-client",
	"license" : "MIT",
	"type" : "library",
	"description" : "Generic PHP API client for Bokbasen's API",
	"homepage" : "https://github.com/Bokbasen/php-api-client",
	"autoload" : {
		"psr-4" : {
			"Bokbasen\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Bokbasen\\ApiClient\\Tests\\" : "tests"
		}
	},
	"authors" : [{
			"name" : "Ketil Stadskleiv",
			"email" : "ketil@bokbasen.no"
		}
	],
	"require" : {
		"php" : ">=5.6",
		"psr/log" : "^1",
		"php-http/client-implementation" : "^1.0",
		"php-http/httplug" : "^1.0",
		"php-http/discovery" : "^1.0",
		"php-http/message" : "^1.0",
		"bokbasen/php-sdk-auth" : "^2"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5",
		"php-http/guzzle6-adapter" : "^1.0"
	}
}
